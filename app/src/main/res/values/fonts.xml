        app:fontProviderAuthority="@string/font_provider_authority"
        app:fontProviderPackage="@string/font_provider_package"
        app:fontProviderQuery="@string/font_provider_query">

        <font
            app:font="@font/inter_regular"
            app:fontWeight="400"
            app:fontStyle="normal"/>

        <font
            app:font="@font/inter_extrabold"
